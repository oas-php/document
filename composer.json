  "description": "Object representation of Open API document",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kuba Biernacki",
      "email": "kubiernacki@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "nikic/iter": "^2.0",
    "psr/http-factory": "^1.0",
    "nikic/php-parser": "^4.10",
    "biera/array-accessor": "1.0.1",
    "oas-php/utils": "dev-master",
    "oas-php/resolver": "dev-master",
    "oas-php/schema": "dev-master",
    "oas-php/validator": "dev-master"
  },
  "suggest": {
    "guzzlehttp/psr7": "^1.6"
  },
  "autoload": {
    "psr-4": {
